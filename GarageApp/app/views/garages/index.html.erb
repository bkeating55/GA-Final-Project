<h1>Garages</h1>
<%= link_to 'New Garage', new_garage_path %>
<table>

  <% @garages.each do |garage| %>
    <tr>
      <td><%=garage.name %></td>
      <td><%= link_to 'View', garage_path(garage)%></td>
      <td><%= link_to 'Edit', edit_garage_path(garage) %></td>
      <td><%= link_to 'Delete', garage_path(garage),
             method: :delete,
             data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
